  "// scripts": {
    "build": "NODE_ENV=production && webpack --display-error-details --config webpack.config.js",
    "clean": "rm -rf ./dist/*",
    "start": "NODE_ENV=development && ./node_modules/.bin/webpack-dev-server --color --config webpack.config.js --progress --port 8080 --hot",
    "postinstall": "mkdir -p ./dist && cp -r ./node_modules/bootstrap/dist/* ./dist/ && cp ./node_modules/jquery/dist/jquery.min.js ./dist/js/",
    "test": "./node_modules/.bin/mocha --compilers js:babel-register --require ./src/test/setup.js \"./src/**/*.spec.js\"",
    "test:watch": "./node_modules/.bin/mocha --watch --compilers js:babel-core/register --require ./src/test/setup.js \"./src/**/*.spec.js\""
  },
