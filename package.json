{
  "name": "reacttemplate",
  "version": "1.0.0",
  "description": "A Simple ReactJS Rapid Prototype Template based on Webpack",
  "keywords": [
    "reactjs",
    "redux",
    "redux-saga",
    "accessibility",
    "react-router",
    "twitter-bootstrap"
  ],
  "main": "index.js",
  "dependencies": {
    "assert": "^1.4.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "bell-on-bundler-error-plugin": "^2.0.0",
    "bootstrap": "^3.3.7",
    "chai": "^3.5.0",
    "conventional-github-releaser": "^1.1.3",
    "copy-webpack-plugin": "^4.0.1",
    "dompurify": "^0.8.9",
    "enzyme": "^2.8.2",
    "es6-promise": "^4.1.0",
    "exports-loader": "^0.6.4",
    "html-loader": "^0.4.5",
    "imports-loader": "^0.7.1",
    "jquery": "^3.2.1",
    "jsdom": "^10.1.0",
    "lodash": "^4.17.4",
    "mocha": "^3.3.0",
    "moment": "^2.18.1",
    "npm-install-webpack-plugin": "^4.0.4",
    "prop-types": "^15.5.8",
    "react": "^15.5.3",
    "react-addons-test-utils": "^15.5.1",
    "react-bootstrap": "^0.30.10",
    "react-data-grid": "^2.0.29",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.0.0",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.0",
    "redux-logger": "^3.0.1",
    "redux-saga": "^0.14.6",
    "redux-thunk": "^2.2.0",
    "should": "^11.2.1",
    "validate.js": "^0.11.1",
    "webpack": "^2.5.1",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "build": "NODE_ENV=production && webpack --display-error-details --config webpack.config.js",
    "clean": "rm -rf ./dist/*",
    "start": "NODE_ENV=development && webpack-dev-server --config webpack.config.js --inline",
    "postinstall": "mkdir ./dist/ && cp -r ./node_modules/bootstrap/dist/* ./dist/ && cp ./node_modules/jquery/dist/jquery.min.js ./dist/js/",
    "test": "./node_modules/.bin/mocha --compilers js:babel-register --require ./src/test/setup.js \"./src/**/*.spec.js\"",
    "test:watch": "./node_modules/.bin/mocha --watch --compilers js:babel-core/register --require ./src/test/setup.js \"./src/**/*.spec.js\""
  },
  "repository": {
    "type": "git",
    "url": "https://psenger@github.com/psenger/ReactJS-Rapid-Prototype-Template.git"
  },
  "author": "Philip A Senger <philip.a.senger@cngrgroup.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/psenger/ReactJS-Rapid-Prototype-Template/issues"
  },
  "homepage": "https://github.com/psenger/ReactJS-Rapid-Prototype-Template",
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0",
      "stage-1"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-object-assign",
      "transform-class-properties"
    ]
  }
}
